             xmlns:local="clr-namespace:ImageFiMan"
             xmlns:src="clr-namespace:ImageFiMan.Models"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ObjectDataProvider x:Key="Photos" ObjectType="{x:Type src:PhotoCollection}"/>
    </Application.Resources>
</Application>
